 var areaOfRectangle=function(num1,num2){
   console.log("area of rectangle is =",num1*num2);
   
 }
areaOfRectangle(499,917);
 
 console.log("========================================================");
 console.log("=========swap the value By using functionExpresstion=========")

 var swapValues=function(val1,val2){
    console.log("--------------Before Swap---------------");
    console.log(val1,val2);
    var temp=val1;
    val1=val2;
    val2=temp;
    console.log("------------------After Swap-------------");
    console.log(val1,val2);
 }
 swapValues("Virat","Anuska");
 swapValues(1000,2000);
