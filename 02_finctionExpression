 var showDetails= "JS the most popular language";
 console.log("Given string is",showDetails);
 console.log("Total charactar of string is:-",showDetails.length);
 console.log("charactar at index 6:-",showDetails.charAt(6));
 console.log("charactar at index 11:-",showDetails.charAt(11));
 
 
 var lengthTotal=showDetails.length;
 console.log("the last charactar at string is:",showDetails.charAt(lengthTotal-1));
 console.log("the first character as string is :", showDetails.charAt(0));
 console.log("length of string is",showDetails.length);
